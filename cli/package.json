{
  "name": "mqttx-cli",
  "version": "1.9.6",
  "description": "MQTTX Command Line Tools",
  "keywords": [
    "mqtt",
    "publish/subscribe",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "mqttx": "./bin/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "~4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "compare-versions": "^6.1.0",
    "concat-stream": "^2.0.0",
    "core-js": "^3.26.0",
    "js-yaml": "^4.1.0",
    "mqtt": "^4.3.7",
    "protobufjs": "^7.2.3",
    "pump": "^3.0.0",
    "readable-stream": "^4.4.2",
    "signale": "^1.4.0",
    "split2": "^4.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/concat-stream": "^2.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.6.4",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^4.0.2",
    "@types/signale": "^1.4.4",
    "@types/split2": "^4.2.0",
    "@types/ws": "^8.5.3",
    "typescript": "^4.9.5"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64",
      "node16-alpine-x64",
      "node16-linux-arm64",
      "node16-macos-arm64",
      "node16-win-arm64",
      "node16-alpine-arm64"
    ],
    "outputPath": "release",
    "scripts": [
      "dist/src/scenarios/**/*"
    ]
  }
}
